{
  "name": "vui-breadcrumbs",
  "version": "2.0.0",
  "description": "Mixins and CSS for applying breadcrumb styles",
  "scripts": {
    "prebuild": "cpy breadcrumbs.html ./bower_components/temp",
    "build": "vulcanize ./bower_components/temp/breadcrumbs.html > ./test/breadcrumbs.test.html",
    "postbuild": "rimraf ./bower_components/temp",
    "clean": "rimraf ./test/breadcrumbs.test.html",
    "demo": "polyserve",
    "postinstall": "bower install",
    "snapshot": "phantomjs ./screenshots/build.js",
    "test": "polylint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Brightspace/valence-ui-breadcrumbs.git"
  },
  "keywords": [
    "d2l",
    "valence-ui",
    "vui"
  ],
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Brightspace/valence-ui-breadcrumbs/issues"
  },
  "homepage": "https://github.com/Brightspace/valence-ui-breadcrumbs",
  "dependencies": {
    "bower": "^1.7.7"
  },
  "devDependencies": {
    "cpy-cli": "^1.0.0",
    "csslint": "^0.10.0",
    "images-to-variables": "^0.3.0",
    "node-sass": "^3.4.2",
    "phantomjs-prebuilt": "^2.1.4",
    "polylint": "^2.10.1",
    "polyserve": "^0.7.1",
    "rimraf": "^2.5.2",
    "vulcanize": "^1.14.8"
  }
}
